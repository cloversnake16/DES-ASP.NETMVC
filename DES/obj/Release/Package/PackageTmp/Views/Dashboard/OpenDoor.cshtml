@model IEnumerable<string>

<h4>Open Door</h4>
<table class="table">
    <tr>
        <td style="border:none" align="right">
            @Html.ActionLink("Return", "Index", "Dashboard", null, new { @class = "btn btn-primary" })
        </td>
    </tr>
    <tr>
        <td style="border:none" align="left">
            @for (int i = 0; i < 128; i++)
            {
                <div class="div-opendoor" title="@Model.ElementAt(i)" style="text-align:center; width:60px; display:inline-block; border: 1px solid #dddddd; padding: 5px; margin:5px">@(i + 1)</div>
            }
        </td>
    </tr>
</table>
